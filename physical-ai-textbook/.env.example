# Frontend Environment Variables
# These are used during build and can be set in GitHub Actions secrets/variables

# Backend API URL (your Render.com deployment)
REACT_APP_BACKEND_URL=https://physical-ai-backend.onrender.com

# Feature Flags (set to 'true' to enable)
REACT_APP_FEATURE_AUTH=false
REACT_APP_FEATURE_QUIZ=false
REACT_APP_FEATURE_PERSONALIZATION=false
REACT_APP_FEATURE_URDU=false
