# Frontend Environment Variables
# These are used during build and can be set in GitHub Actions secrets/variables

# Backend API URL (your Render.com deployment)
REACT_APP_BACKEND_URL=http://127.0.0.1:8000

# Feature Flags (set to 'true' to enable)
REACT_APP_FEATURE_AUTH=true
REACT_APP_FEATURE_QUIZ=true
REACT_APP_FEATURE_PERSONALIZATION=true
REACT_APP_FEATURE_URDU=true
